{
  "dashboard": {
    "title": "NodeCG Next - Overview",
    "tags": ["nodecg", "overview"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(nodecg_http_requests_total[5m])",
            "legendFormat": "{{method}} {{route}}"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "HTTP Request Duration (p95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(nodecg_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{route}}"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
      },
      {
        "id": 3,
        "title": "Active WebSocket Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "nodecg_websocket_connections",
            "legendFormat": "{{namespace}}"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
      },
      {
        "id": 4,
        "title": "Replicant Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(nodecg_replicant_operations_total[5m])",
            "legendFormat": "{{operation}} - {{namespace}}"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
      },
      {
        "id": 5,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "nodecg_system_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "Memory (MB)"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 }
      },
      {
        "id": 6,
        "title": "Bundles Loaded",
        "type": "stat",
        "targets": [
          {
            "expr": "nodecg_bundles_loaded"
          }
        ],
        "gridPos": { "h": 4, "w": 6, "x": 12, "y": 16 }
      },
      {
        "id": 7,
        "title": "Active Replicants",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(nodecg_active_replicants)"
          }
        ],
        "gridPos": { "h": 4, "w": 6, "x": 18, "y": 16 }
      }
    ]
  }
}
